{
    "name": "powermodesounds",
    "displayName": "PowerModeSounds",
    "description": "An explosive way to code.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.activatePowerModeSounds"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.activatePowerModeSounds",
                "title": "Active Power Mode Sounds"
            },
            {
                "command": "extension.deactivatePowerModeSounds",
                "title": "Deactivate Power Mode Sounds"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.1",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.42.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "iohook": "^0.6.5",
        "mocha": "^7.0.1",
        "typescript": "^3.7.5",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "iohook": "^0.6.5"
    },
    "prettier": {
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true
    }
}
